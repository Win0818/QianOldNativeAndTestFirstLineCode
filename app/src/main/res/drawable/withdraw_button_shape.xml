<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--填充颜色-->
    <solid android:color="@color/orangered" />

    <!--设置按钮的四个角为弧形-->
    <!--android:radius 弧形半径-->
    <corners android:radius="8dip" />
    <!--padding-->
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp"/>
</shape>